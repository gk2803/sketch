{
  "ecmaVersion": 8,
  "libs": [
    "underscore",
    "react",
    "jquery",
    "chai",
    "browser"
  ],
  "loadEagerly": [],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "doc_comment": true,
    "commonjs": {},
    "requirejs": {
      "baseURL": "",
      "paths": "",
      "override": ""
    },
    "angular": {},
    "es_modules": {},
    "node_resolve": {},
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "complete_strings": {
      "maxLength": 15
    },
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    }
  }
}